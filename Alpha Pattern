import java.util.Scanner;
public class Solution {

	public static void main(String[] args) {
		
		 Scanner s=new Scanner(System.in);
     int n=s.nextInt();
     int i=1;
     int p=1;
     while(i<=n){
         
         int j=1;
         p=i;
         while(j<=i){
             
             char jthchar=(char)('A'+p-1);
             System.out.print(jthchar);
             
             j=j+1;
         }
         System.out.println();
         i=i+1;
     }
	}

}
