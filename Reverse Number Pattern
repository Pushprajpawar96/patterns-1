import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in); 
        int rows=sc.nextInt();
        System.out.println("");
        for (int i=1; i<=rows; i++){
        for (int j=i; j>=1; j--){
  
        System.out.print(j);
}
        System.out.println();
}

		
	}

}
